        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
   </div>

    <div class="circular-active" style="display: flex;margin-left:1%;">
      <div style="width: 30%; padding: 1em;">
        <a href="/face">
          <img src="https://cdn.shopify.com/s/files/1/0586/9187/9111/files/ic_face_300x_crop_center.png?v=1639605133"
               alt="Face" style="width: 100%;" />
        </a>
      </div>
      <div style="width: 30%; padding: 1em;">
        <a href="/oil">
          <img src="https://cdn.shopify.com/s/files/1/0586/9187/9111/files/ic_oil_300x_crop_center.png?v=1639605133"
           alt="oil" style="width: 100%;"  />
        </a>
      </div>
      <div style="width: 30%; padding: 1em;">
        <a href="/lips">
          <img src="https://cdn.shopify.com/s/files/1/0586/9187/9111/files/ic_lips_300x_crop_center.png?v=1639605133"
          alt="Lips" style="width: 100%;"  />
        </a>
      </div>
      <div style="width: 30%; padding: 1em;">
        <a href="/body">
          <img src="https://cdn.shopify.com/s/files/1/0586/9187/9111/files/ic_body_300x_crop_center.png?v=1639605133"
          alt="Body" style="width: 100%;"  />
        </a>
      </div>

     

    </div>

   
    <div class="row">
        <% for(var i=0;i<products.length;i++) {%>
          <div class="col-4" style="padding: 3%;">
            <div><h5><b><%=products[i].name %></b></h5></div>
            <div>
              <img
                  src="/images/uploaded/<%=products[i].image %>"
                  alt=""
                  width="70%"
                />
            </div>
            <div>
              <div>
                <p><b><%=products[i].description %></b></p>
                <p><b>Price: </b><%=products[i].price %></p>
                <p><b>Color: </b><%=products[i].color %></p>
              </div>
              <a href="/add-cart/<%=products[i]._id %>" class="btn btn-info w-full"
                >Add To Cart</a
              >
            </div>
          </div>
          <% } %>

        </products.length;i++)>
    </div>
  </div>
  
    <div class="container">
